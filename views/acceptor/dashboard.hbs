
<nav class="navbar navbar-light bg-danger">
    <a href='/acceptor' class="navbar-brand mb-0 h1">Acceptor Dashboard</a>
    <a href='/logout' class="btn btn-danger">Logout</a>
</nav>

<table class="table mt-5">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Total Food</th>
            <th scope="col">Waste Food</th>
            <th scope="col">Pickup Time</th>
          
           
        </tr>
    </thead>
    <tbody>
       
        {{#each response}}
        <tr>
            <th scope="row">{{@index}}</th>
            <td>{{this.totalFoodCooked}}</td>
            <td>{{this.foodWasteQty}}</td>
            <td>{{this.pickUpTime}}</td>
            <td><a href="/profile/{{this.donorId}}" class="btn btn-primary">Donor Profile</a></td>
            
          
           
        </tr>
        {{/each}}

    </tbody>
</table>